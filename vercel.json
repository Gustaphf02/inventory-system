{
  "version": 2,
  "functions": {
    "public/index.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "public/**/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/public/index.php"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/public/index.php"
    },
    {
      "src": "/products",
      "dest": "/public/index.php"
    },
    {
      "src": "/categories",
      "dest": "/public/index.php"
    },
    {
      "src": "/suppliers",
      "dest": "/public/index.php"
    },
    {
      "src": "/departments",
      "dest": "/public/index.php"
    },
    {
      "src": "/locations",
      "dest": "/public/index.php"
    },
    {
      "src": "/inventory/summary",
      "dest": "/public/index.php"
    },
    {
      "src": "/upload-photo",
      "dest": "/public/index.php"
    },
    {
      "src": "/product-history",
      "dest": "/public/index.php"
    },
    {
      "src": "/health",
      "dest": "/public/index.php"
    },
    {
      "src": "/(.*\\.php)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
