{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "api/login.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "api/logout.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "api/register.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "rewrites": [
    {
      "source": "/login.php",
      "destination": "/api/login.php"
    },
    {
      "source": "/logout.php",
      "destination": "/api/logout.php"
    },
    {
      "source": "/register.php",
      "destination": "/api/register.php"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/index.php"
    },
    {
      "source": "/auth/:path*",
      "destination": "/api/index.php"
    },
    {
      "source": "/products",
      "destination": "/api/index.php"
    },
    {
      "source": "/categories",
      "destination": "/api/index.php"
    },
    {
      "source": "/suppliers",
      "destination": "/api/index.php"
    },
    {
      "source": "/departments",
      "destination": "/api/index.php"
    },
    {
      "source": "/locations",
      "destination": "/api/index.php"
    },
    {
      "source": "/inventory/summary",
      "destination": "/api/index.php"
    },
    {
      "source": "/upload-photo",
      "destination": "/api/index.php"
    },
    {
      "source": "/product-history",
      "destination": "/api/index.php"
    },
    {
      "source": "/health",
      "destination": "/api/index.php"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
