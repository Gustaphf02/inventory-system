{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "api/login.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "api/logout.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "api/register.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/login.php",
      "dest": "/api/login.php",
      "continue": false
    },
    {
      "src": "/logout.php",
      "dest": "/api/logout.php",
      "continue": false
    },
    {
      "src": "/register.php",
      "dest": "/api/register.php",
      "continue": false
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/products",
      "dest": "/api/index.php"
    },
    {
      "src": "/categories",
      "dest": "/api/index.php"
    },
    {
      "src": "/suppliers",
      "dest": "/api/index.php"
    },
    {
      "src": "/departments",
      "dest": "/api/index.php"
    },
    {
      "src": "/locations",
      "dest": "/api/index.php"
    },
    {
      "src": "/inventory/summary",
      "dest": "/api/index.php"
    },
    {
      "src": "/upload-photo",
      "dest": "/api/index.php"
    },
    {
      "src": "/product-history",
      "dest": "/api/index.php"
    },
    {
      "src": "/health",
      "dest": "/api/index.php"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
